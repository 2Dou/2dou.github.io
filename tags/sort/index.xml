&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="k">if&lt;/span> &lt;span class="nx">intervals&lt;/span>&lt;span class="p">[&lt;/span>&lt;span class="nx">rStart&lt;/span>&lt;span class="p">].&lt;/span>&lt;span class="nx">End&lt;/span> &lt;span class="p">&amp;gt;&lt;/span> &lt;span class="nx">newInterval&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">End&lt;/span> &lt;span class="p">{&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="c1">// 将当前区间与 newInterval 合并&lt;/span>
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="nx">newInterval&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">End&lt;/span> &lt;span class="p">=&lt;/span> &lt;span class="nx">intervals&lt;/span>&lt;span class="p">[&lt;/span>&lt;span class="nx">rStart&lt;/span>&lt;span class="p">].&lt;/span>&lt;span class="nx">End&lt;/span>
